{
  "manifest_version": 3,
  "name": "Header Filter Extension",
  "version": "1.0",
  "permissions": [
    "cookies",
    "activeTab",
    "scripting",
    "webNavigation",
    "webRequest",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  }
}
